# GAS API URL（Google Apps Scriptのデプロイ後に発行されるURL）
GAS_API_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec

# GAS API KEY（GASスクリプトプロパティの API_KEY に設定した値）
GAS_API_KEY=YOUR_GAS_API_KEY

# 管理者ログイン情報
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your_secure_password

# セッション署名用シークレット（ランダムな長い文字列を設定）
SESSION_SECRET=your_random_session_secret

# Database
DATABASE_URL=postgresql://postgres:password@db:5432/tenko_system_development

# シフト入力用GoogleスプレッドシートURL
SPREADSHEET_URL=https://docs.google.com/spreadsheets/d/YOUR_SPREADSHEET_ID/
